<div class="right_col" role="main">
  <table class="table table-bordered table-striped">
    <thead>
    <tr>
      <th>Computer name</th>
      <th>Domain name</th>
      <th>Time</th>
      <th>Protol</th>
      <th>Is TCP?</th>
    </tr>
    </thead>
    <tbody>
    <% @packages.each do |package| %>
        <tr>
          <td><%= package.from %></td>
          <td><%= package.to %></td>
          <td><%= package.time %></td>
          <td><%= package.protocol %></td>
          <td><%= package.is_tcp %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
